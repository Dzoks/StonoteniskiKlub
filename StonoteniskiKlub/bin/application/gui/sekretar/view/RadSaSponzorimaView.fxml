<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane onMouseClicked="#deselect" prefHeight="400.0" prefWidth="730.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.gui.sekretar.controller.RadSaSponzorimaController">
   <children>
      <TableView fx:id="tblSponzori" layoutX="14.0" layoutY="49.0" prefHeight="295.0" prefWidth="700.0">
         <columns>
            <TableColumn fx:id="colNaziv" editable="false" prefWidth="150.0" text="Naziv" />
            <TableColumn fx:id="colAdresa" editable="false" prefWidth="200.0" text="Adresa" />
            <TableColumn fx:id="colMail" editable="false" prefWidth="150.0" text="E-mail adresa" />
            <TableColumn fx:id="colTelefon" editable="false" prefWidth="100.0" text="Telefon" />
            <TableColumn fx:id="colAktivan" editable="false" prefWidth="100.0" text="Ugovor aktivan" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <RadioButton fx:id="rbSvi" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#prikaziSve" selected="true" text="Svi">
         <toggleGroup>
            <ToggleGroup fx:id="grpSvi" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rbAktivni" layoutX="63.0" layoutY="14.0" mnemonicParsing="false" onAction="#prikaziAktivne" text="Aktivni" toggleGroup="$grpSvi" />
      <RadioButton fx:id="rbNeaktivni" layoutX="133.0" layoutY="14.0" mnemonicParsing="false" onAction="#prikaziNeaktivne" text="Neaktivni" toggleGroup="$grpSvi" />
      <Button fx:id="btnPretrazi" layoutX="649.0" layoutY="10.0" mnemonicParsing="false" onAction="#pretrazi" text="Pretrazite" />
      <TextField fx:id="txtPretraga" layoutX="493.0" layoutY="10.0" />
      <ComboBox fx:id="cbPretraga" layoutX="384.0" layoutY="10.0" prefWidth="100.0" />
      <Label layoutX="309.0" layoutY="14.0" text="Pretrazite po" />
      <Button fx:id="btnUgovori" layoutX="14.0" layoutY="355.0" mnemonicParsing="false" onAction="#pregledajUgovore" text="Pregledajte ugovore" />
      <Button fx:id="btnDodajSponzora" layoutX="604.0" layoutY="355.0" mnemonicParsing="false" onAction="#dodajSponzora" text="Dodajte sponzora" />
      <Button fx:id="btnAzuriraj" layoutX="143.0" layoutY="355.0" mnemonicParsing="false" onAction="#azuriraj" prefHeight="25.0" prefWidth="71.0" text="Azurirajte" />
      <Button fx:id="btnObrisi" layoutX="220.0" layoutY="355.0" mnemonicParsing="false" onAction="#obrisi" prefHeight="25.0" prefWidth="71.0" text="Obrisite " />
      <Button fx:id="btnDodajUgovor" layoutX="482.0" layoutY="355.0" mnemonicParsing="false" onAction="#dodajUgovor" prefHeight="25.0" prefWidth="110.0" text="Dodajte ugovor" />
   </children>
</AnchorPane>
